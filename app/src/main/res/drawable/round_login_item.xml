<?xml version="1.0" encoding="UTF-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#dc8504" />
    <stroke
        android:width="0dp"
        android:color="#dc8504" />
    <gradient
    android:startColor="#dc8504"
    android:endColor="#dc8504"
    android:angle="90"/>

    <padding
        android:bottom="5dp"
        android:left="25dp"
        android:right="25dp"
        android:top="5dp" />
    <corners
        android:bottomLeftRadius="@dimen/round_edge"
        android:bottomRightRadius="@dimen/round_edge"
        android:topLeftRadius="@dimen/round_edge"
        android:topRightRadius="@dimen/round_edge" />
</shape>

