<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nima.chatkit.sample">

    <uses-permission android:name="android.permission.INTERNET" />
    <application

        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/GreenTheme">
        <meta-data android:name="firebase_crashlytics_collection_enabled" android:value="false"/>

        <activity
            android:name="com.nima.chatkit.sample.Controler.LoginActivity"
            android:theme="@style/BlueThemeNoActionBar">

        </activity>
        <activity
            android:name="com.nima.chatkit.sample.features.demo.def.DefaultMessagesActivity"
            android:theme="@style/BlueTheme" />
        <activity
            android:name="com.nima.chatkit.sample.features.demo.def.DefaultDialogsActivity"
            android:theme="@style/BlueTheme" />
        <activity android:name="com.nima.chatkit.sample.features.demo.styled.StyledMessagesActivity" />
        <activity android:name="com.nima.chatkit.sample.features.demo.styled.StyledDialogsActivity" />
        <activity android:name="com.nima.chatkit.sample.features.demo.custom.layout.CustomLayoutDialogsActivity" />
        <activity android:name="com.nima.chatkit.sample.features.demo.custom.layout.CustomLayoutMessagesActivity" />
        <activity android:name="com.nima.chatkit.sample.features.demo.custom.holder.CustomHolderDialogsActivity" />
        <activity android:name="com.nima.chatkit.sample.features.demo.custom.holder.CustomHolderMessagesActivity" />
        <activity
            android:name="com.nima.chatkit.sample.features.demo.custom.media.CustomMediaMessagesActivity"
            android:theme="@style/BlueTheme" />
        <activity
            android:name="com.nima.chatkit.sample.splashActivity"
            android:theme="@style/splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>